  // Here we take the price for ingredient and add it to total price of pizza\n        return pizza.totalPrice + INGREDIENT_PRICE[ingredientType];\n\n      case \"REMOVE\":\n        return pizza.totalPrice - INGREDIENT_PRICE[ingredientType];\n    }\n  };\n\n  const addIngredient = (type) => {\n    const updatedCount = pizza.ingredients[type] + 1; // we take old count and add 1 to it\n    const updatedIngredients = { ...pizza.ingredients };\n    updatedIngredients[type] = updatedCount;\n    setPizza({\n      ingredients: updatedIngredients,\n      totalPrice: calculatePrice(type, \"ADD\"),\n    });\n  };\n\n  const removeIngredient = (type) => {\n    if (pizza.ingredients[type] <= 0) return;\n    const updatedCount = pizza.ingredients[type] - 1;\n    const updatedIngredients = { ...pizza.ingredients };\n    updatedIngredients[type] = updatedCount;\n    setPizza({\n      ingredients: updatedIngredients,\n      totalPrice: calculatePrice(type, \"REMOVE\"),\n    });\n  };\n  return (\n    <>\n      <Pizza ingredients={pizza.ingredients} />\n      <BuildControls\n        ingredientAdded={addIngredient}\n        ingredientRemoved={removeIngredient}\n      />\n    </>\n  );\n};\n\nexport default PizzaBuilder;\n","/Users/Olena/Documents/Projects/React_diff_projects/Build-O-Pizza/build-o-pizza/src/components/Pizza/BuildControls/BuildControl/ButtonRemove/ButtonRemove.js",[],["111","112"],"/Users/Olena/Documents/Projects/React_diff_projects/Build-O-Pizza/build-o-pizza/src/components/Pizza/BuildControls/BuildControl/StyledButton/StyledButton.js",[],"/Users/Olena/Documents/Projects/React_diff_projects/Build-O-Pizza/build-o-pizza/src/components/Pizza/PizzaIngredient/Sauce/Sauce.js",[],["113","114"],"/Users/Olena/Documents/Projects/React_diff_projects/Build-O-Pizza/build-o-pizza/src/components/Pizza/PizzaIngredient/Cheese/Cheese.js",[],"/Users/Olena/Documents/Projects/React_diff_projects/Build-O-Pizza/build-o-pizza/src/components/Pizza/PizzaIngredient/GreenPepper/GreenPepper.js",[